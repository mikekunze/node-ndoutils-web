                .navbar-header
                    button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    a.navbar-brand(href='/#/') node-web-skel
                .navbar-collapse.collapse
                    ul.nav.navbar-nav
                        if name == "index"
                            li.active
                                a(href="#/home", data-name="index", data-component="view", data-method="getViewByName") Home
                        else
                            li
                                a(href="#/home", data-name="index", data-component="view", data-method="getViewByName") Home

                        if name == "ndoutils"
                            li.active
                                a(href="#/ndoutls", data-name="ndoutils", data-component="view", data-method="getViewByName") ndoutils
                        else
                            li
                                a(href="#/ndoutils", data-name="ndoutils", data-component="view", data-method="getViewByName") ndoutils

